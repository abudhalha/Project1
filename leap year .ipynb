   "source": [
    "import calendar\n",
    "year = int(input('enter the year='))\n",
    "\n",
    "#calling is leap() method to verify\n",
    "val = calendar.isleap(year)\n",
    "\n",
    "#checking the condition is true or not\n",
    "\n",
    "if val==True:\n",
    "    print('% s is a leap year'% year)\n",
    "else:\n",
    "    print('% s is not a leap year'% year)"
   ]
